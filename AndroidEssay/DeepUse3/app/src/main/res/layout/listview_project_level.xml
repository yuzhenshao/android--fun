<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/iv_dj_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textSize="16sp"
            android:textColor="@color/color_606266"
            android:layout_marginLeft="@dimen/app_24dp"
            android:text=""/>

        <ImageView
            android:id="@+id/iv_dj_item_select"
            android:layout_width="@dimen/app_24dp"
            android:layout_height="@dimen/app_24dp"
            android:layout_marginRight="@dimen/app_20dp"
            android:src="@mipmap/regi_weixuan"/>

    </LinearLayout>

</LinearLayout>