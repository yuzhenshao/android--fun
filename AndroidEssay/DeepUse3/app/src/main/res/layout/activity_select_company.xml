<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/color_ffffff"
    android:layout_height="match_parent">

    <include layout="@layout/main_title"/>

    <TextView
        android:id="@+id/tv_sel_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="@color/color_303133"
        android:layout_marginLeft="@dimen/app_24dp"
        android:layout_marginTop="@dimen/app_35dp"
        android:text="请您选择企业/团队"/>

    <ListView
        android:id="@+id/sel_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/color_edeff2"
        android:dividerHeight="@dimen/app_1dp"
        android:layout_marginTop="@dimen/app_20dp"/>

</LinearLayout>