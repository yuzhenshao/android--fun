                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/app_5dp"
                                android:text="相关图片"
                                android:textColor="@color/color_303133"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/app_24dp">

                            <android.support.v7.widget.RecyclerView
                                android:id="@+id/clxx_recycleview"
                                android:layout_width="match_parent"
                                android:layout_marginLeft="@dimen/app_10dp"
                                android:layout_marginRight="@dimen/app_10dp"
                                android:layout_marginBottom="@dimen/app_20dp"
                                android:layout_marginTop="@dimen/app_5dp"
                                android:layout_height="160dp"/>

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout style="@style/newly_ll_st">
                        <ImageView
                            android:layout_width="6dp"
                            android:layout_height="6dp"
                            android:src="@drawable/shape_blue_corner4"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/app_5dp"
                            android:text="客户签名"
                            android:textColor="@color/color_303133"
                            android:textSize="16sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/ll_dzqm"
                        android:layout_width="match_parent"
                        android:layout_height="179dp"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="@dimen/app_24dp"
                        android:layout_marginRight="@dimen/app_24dp"
                        android:paddingLeft="@dimen/app_24dp"
                        android:background="@drawable/shouhou_dzqm_shape"
                        android:paddingRight="@dimen/app_24dp">

                        <ImageView
                            android:layout_margin="3dp"
                            android:id="@+id/iv_qm"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </LinearLayout>



</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>