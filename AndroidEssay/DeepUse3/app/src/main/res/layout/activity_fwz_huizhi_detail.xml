                                android:layout_marginLeft="@dimen/app_5dp"
                                android:text="相关图片"
                                android:textColor="@color/color_303133"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/app_24dp">

                            <android.support.v7.widget.RecyclerView
                                android:id="@+id/jjfa_recycleview"
                                android:layout_width="match_parent"
                                android:layout_marginLeft="@dimen/app_10dp"
                                android:layout_marginRight="@dimen/app_10dp"
                                android:layout_marginBottom="@dimen/app_20dp"
                                android:layout_marginTop="@dimen/app_5dp"
                                android:layout_height="160dp"/>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_ywg"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/app_10dp"
                    android:layout_marginTop="@dimen/app_10dp"
                    android:background="@color/color_ffffff"
                    android:orientation="vertical">

                    <LinearLayout style="@style/newly_ll_st">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/small_huidan" />
                        <TextView
                            android:id="@+id/tv_orderno1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="yzs201903130001"
