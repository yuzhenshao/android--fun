                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll_clz"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="@dimen/app_10dp"
                    android:layout_marginTop="@dimen/app_10dp"
                    android:background="@color/color_ffffff"
                    android:orientation="vertical">

                    <LinearLayout style="@style/newly_ll_st">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@mipmap/small_huidan" />
                        <TextView
                            android:id="@+id/tv_orderno"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="yzs201903130001"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/color_303133"
                            android:textSize="16sp" />
                        <TextView
                            android:id="@+id/tv_huidan_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="处理中回单"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/color_303133"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/clz_huizhi"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <LinearLayout style="@style/newly_ll_st">

                            <ImageView
                                android:layout_width="6dp"
                                android:layout_height="6dp"
                                android:src="@drawable/shape_blue_corner4"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/app_5dp"
                                android:text="故障分析"
                                android:textColor="@color/color_303133"
                                android:textSize="16sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
