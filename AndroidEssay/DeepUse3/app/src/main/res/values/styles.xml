        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="newly_et_st2" parent="newly_et_st">
        <item name="android:focusableInTouchMode">false</item>
    </style>


    <style name="newly_tx">
        <item name="android:layout_width">0dp</item>
