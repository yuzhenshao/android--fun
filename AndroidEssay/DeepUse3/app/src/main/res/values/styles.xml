    <style name="newly_et_st">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textColorHint">@color/color_BFC2CC</item>
        <item name="android:paddingLeft">@dimen/app_10dp</item>
        <item name="android:paddingRight">@dimen/app_24dp</item>
        <item name="android:textSize">16sp</item><!-- 文字大小-->
        <item name="android:textColor">@color/color_303133</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="customer_et_st">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
