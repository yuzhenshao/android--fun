        <item name="android:gravity">right|center_vertical</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="survey_iv">
        <item name="android:layout_width">@dimen/app_20dp</item>
        <item name="android:layout_height">@dimen/app_20dp</item>
        <item name="android:layout_marginRight">@dimen/app_20dp</item>
    </style>

