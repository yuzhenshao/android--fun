        <item name="tabIndicatorHeight">3dp</item> <!-- 指示线高度-->
        <item name="tabSelectedTextColor">@color/color_3D7EFF</item> <!-- 文字被选中的颜色-->
    </style>

    <style name="MyTabText2" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">15sp</item><!-- 文字大小-->
        <item name="android:textColor">@color/color_303133</item>
    </style>


