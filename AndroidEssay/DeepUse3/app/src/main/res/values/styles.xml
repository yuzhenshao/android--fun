        <item name="android:textColor">@color/color_303133</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <!--指派人员-->
    <style name="appoint_tv_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/app_30dp</item>
        <item name="android:paddingLeft">@dimen/app_24dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_303133</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="appoint_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/color_ffffff</item>
