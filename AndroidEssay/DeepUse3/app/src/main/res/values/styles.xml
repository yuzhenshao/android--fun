<resources>

    <!-- Base application theme. -->
    <!--<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">-->
    <!--&lt;!&ndash; Customize your theme here. &ndash;&gt;-->
    <!--<item name="colorPrimary">@color/colorPrimary</item>-->
    <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
    <!--<item name="colorAccent">@color/colorAccent</item>-->
    <!--</style>-->

    <style name="AppTheme" parent="QMUI.Compat.NoActionBar">
        <!-- 配置qmui提供的theme -->
        <item name="qmui_config_color_blue">@color/colorPrimary</item>

        <!-- 配置qmui dialog最小显示长宽 -->
        <item name="qmui_tip_dialog_min_width">80dp</item>
        <item name="qmui_tip_dialog_min_height">80dp</item>
    </style>

    <!--圆角图片-->
    <declare-styleable name="RoundImageView">
        <attr name="rect_adius" format="float" />
    </declare-styleable>


    <style name="PayDialogStyle" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:windowAnimationStyle">@style/PayDialogAnim</item>-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="PayDialogAnim">
        <item name="android:windowEnterAnimation">@anim/pay_dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/pay_dialog_exit</item>
    </style>


    <!--工作-->
    <style name="work_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/app_24dp</item>
        <item name="android:layout_marginRight">@dimen/app_24dp</item>
        <item name="android:layout_marginTop">@dimen/app_15dp</item>
    </style>

    <style name="kanban_ll_bg">
        <item name="android:layout_width">@dimen/app_95dp</item>
        <item name="android:layout_height">@dimen/app_92dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/yinying_bg_shape</item>
    </style>

    <style name="shenpi_ll_bg">
        <item name="android:layout_width">@dimen/app_56dp</item>
        <item name="android:layout_height">@dimen/app_57dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="shenpi_iv">
        <item name="android:layout_width">@dimen/app_36dp</item>
        <item name="android:layout_height">@dimen/app_36dp</item>
    </style>

    <style name="shenpi_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_606266</item>
    </style>

    <!--新建项目-->
